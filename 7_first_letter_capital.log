function capital(x){
    var arr = x.split(" ");
    for(var i = 0; i < arr.length; i++){
        arr[i]  = arr[i].charAt(0).toUpperCase() + arr[i].slice(1);
    }
    var str = arr.join(" ");
    console.log(str);
}
